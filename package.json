{
  "name": "cypressautomation",
  "version": "1.0.0",
  "description": "Cypress Automation",
  "main": "index.js",
  "scripts": {
    "runTCWOTestRunner": "node_modules/.bin/cypress run",
    "openTestRunner": "node_modules/.bin/cypress open",
    "runTCHeadedMode": "npm run runTCWOTestRunner -- --headed --browser chrome",
    "runTCHeadlessMode": "npm run runTCWOTestRunner -- --browser chrome",
    "runTCHeadedWithDashBoard": "npm run runTCWOTestRunner -- --headed --record --key e222aeb4-580b-498b-b717-a205f7f36616",
    "runTCWithMochaAwesomeDashBoard": "npm run runTCWOTestRunner -- --reporter mochawesome --headed --record --key e222aeb4-580b-498b-b717-a205f7f36616"
  },
  "author": "Kumar Amit Bhabra",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^3.8.0",
    "mocha": "^5.2.0",
    "mochawesome": "^4.1.0"
  }
}
